{
    "compilerOptions": {
        "module": "esnext",
        "moduleResolution": "node",
        "target": "es6",
        "outDir": "configViewer",
        "lib": [
            "es6",
            "dom"
        ],  
        "jsx": "react",
        "sourceMap": true,
        "rootDir": "..",
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true
    },
    "include": [
		"../../../images.d.ts",
		"src/*"
	],
    "exclude": [
        "node_modules"
    ]
}