{
  "name": "jarvis",
  "displayName": "Jarvis",
  "description": "talking virtual assistant mimicking jarvis and provide useful information to user",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jarvis.helloWorld",
    "onCommand:jarvis.Jarvis"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jarvis.helloWorld",
        "category": "Jarvis",
        "title": "Hello World"
      },
      {
        "command": "jarvis.Jarvis",
        "category": "Jarvis",
        "title": "Jarvis"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm-run-all compile:*",
    "watch": "npm-run-all -p watch:*",
    "idk":"webpack --watch",
    "compile:extension": "tsc -p ./",
    "compile:views": "webpack --mode development",
    "watch:extension": "tsc -watch -p ./",
    "watch:views": "webpack --watch --mode development",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "save-dev": "^0.0.1-security",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "css-loader": "^6.7.3",
    "style-loader": "^3.3.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10"
  }
}
